{
  "name": "@rilekstack/vite-plugin-tanstack-router-file-based",
  "version": "0.0.1-development",
  "description": "A template for creating npm packages using TypeScript and VSCode",
  "files": [
    "lib/**/*"
  ],
  "main": "./lib/index.cjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test:watch": "vitest --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "dev": "unbuild --stub"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rilek/vite-plugin-tanstack-router-file-based.git"
  },
  "license": "MIT",
  "author": "Rafal Ileczko <rilek@users.noreply.github.com> (https://github.com/rilek)",
  "bugs": {
    "url": "https://github.com/rilek/vite-plugin-tanstack-router-file-based/issues"
  },
  "homepage": "https://github.com/rilek/vite-plugin-tanstack-router-file-based#readme",
  "dependencies": {
    "lodash": "^4.17.21",
    "prettier": "^3.2.5",
    "unbuild": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@rilek/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "build",
                "section": "Dependencies and Other Build Updates",
                "hidden": false
              }
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "directories": {
    "test": "test"
  }
}